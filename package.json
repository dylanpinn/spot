{
  "name": "@dylanpinn/spot",
  "version": "1.10.1",
  "author": "Dylan Pinn",
  "bin": {
    "spot": "./bin/run"
  },
  "bugs": "https://github.com/dylanpinn/spot/issues",
  "dependencies": {
    "@oclif/command": "^1.8.22",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "assert-never": "^1.2.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.1",
    "js-yaml": "^4.1.0",
    "qs": "^6.11.1",
    "randomstring": "^1.2.3",
    "ts-morph": "^9.0.0",
    "typescript": "^4.9.5",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@stoplight/spectral": "^5.9.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.2",
    "@types/jest": "^27.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/moxios": "^0.4.15",
    "@types/qs": "^6.9.7",
    "@types/randomstring": "^1.1.8",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.14",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "core-js": "^3.29.1",
    "css-loader": "^6.7.3",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.6.3",
    "jest-junit": "^15.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "mobx": "^6.9.0",
    "nock": "^13.3.0",
    "prettier": "^2.8.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redoc": "^2.0.0",
    "styled-components": "^5.3.9",
    "supertest": "^6.3.3",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.4.2",
    "webpack": "^5.84.0",
    "webpack-cli": "^5.0.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "bin",
    "build",
    "index.d.ts",
    "index.js",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "homepage": "https://github.com/dylanpinn/spot",
  "license": "MIT",
  "oclif": {
    "commands": "./build/cli/src/commands",
    "bin": "spot",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "dylanpinn/spot",
  "scripts": {
    "build-docs": "webpack --config ./docs/webpack.config.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf build; tsc && oclif-dev manifest && yarn build-docs && oclif-dev readme",
    "test": "jest -w 4",
    "ci:test": "jest --config=jest.ci.config.js --ci -w 4",
    "lint:check": "yarn prettier:check && yarn eslint:check",
    "eslint:check": "eslint . --ext .js,.ts,.tsx",
    "prettier:check": "prettier --list-different \"**/*.js\" \"**/*.ts\" \"**/*.tsx\"",
    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "eslint:fix": "eslint . --fix --ext .js,.ts,.tsx",
    "prettier:fix": "prettier --write \"**/*.js\" \"**/*.ts\" \"**/*.tsx\""
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@dylanpinn:registry": "https://npm.pkg.github.com"
  }
}
